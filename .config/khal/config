[calendars]

[[work_local]]
path = ~/.local/share/khal/calendars/work/
type = calendar 

[locale]
timeformat = %H:%M
dateformat = %d/%m/%Y
longdateformat = %d/%m/%Y
datetimeformat = %d/%m/%Y %H:%M
longdatetimeformat = %d/%m/%Y %H:%M

[view]
agenda_event_format =  "{start-time} {end-time} {title}"

[default]
default_calendar = work_local

